import Syn.plumbing
import Syn.plumber
import Syn.log

import sys

argstack = sys.argv

if len(argstack) <= 1:
	Syn.log.l(Syn.log.CRITICAL,"Too few args!")
	argstack.append(None)

Syn.plumber.runRoute(argstack[1], argstack)
